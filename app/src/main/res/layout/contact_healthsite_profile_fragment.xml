<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:paddingHorizontal="20dp"
    android:paddingTop="30dp">

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/contact_healthsite_profile_address_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:background="@color/White"
        app:boxBackgroundColor="@color/White"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/contact_healthsite_profile_address"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:enabled="false"
            android:hint="@string/address"
            android:overScrollMode="ifContentScrolls"
            android:textSize="16sp"
            android:textColor="@android:color/black"/>
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/contact_healthsite_profile_webSite_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:background="@color/White"
        app:boxBackgroundColor="@color/White"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@id/contact_healthsite_profile_address_layout"
        app:layout_constraintStart_toEndOf="@id/contact_healthsite_profile_address_layout">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/contact_healthsite_profile_webSite"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:enabled="false"
            android:hint="@string/website"
            android:maxLines="1"
            android:textSize="16sp"
            android:lines="1"
            android:textColor="@android:color/black"/>
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/contact_healthsite_profile_webMail_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:background="@color/White"
        app:boxBackgroundColor="@color/White"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@id/contact_healthsite_profile_webSite_layout"
        app:layout_constraintStart_toEndOf="@id/contact_healthsite_profile_webSite_layout">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/contact_healthsite_profile_webMail"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:enabled="false"
            android:hint="@string/webmail"
            android:textSize="16sp"
            android:maxLines="1"
            android:lines="1"
            android:textColor="@android:color/black"/>
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/contact_healthsite_profile_phone_number_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:background="@color/White"
        app:boxBackgroundColor="@color/White"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@id/contact_healthsite_profile_webMail_layout"
        app:layout_constraintStart_toEndOf="@id/contact_healthsite_profile_webMail_layout">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/contact_healthsite_profile_phone_number"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:enabled="false"
            android:textSize="16sp"
            android:hint="@string/phone_number"
            android:maxLines="1"
            android:lines="1"
            android:textColor="@android:color/black"/>
    </com.google.android.material.textfield.TextInputLayout>
</androidx.constraintlayout.widget.ConstraintLayout>